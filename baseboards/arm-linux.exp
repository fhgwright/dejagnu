process_multilib_options ""

load_generic_config "linux-gdbserver"

set RSH /usr/bin/ssh
set RCP /usr/bin/scp

set_board_info rsh_prog /usr/bin/ssh
set_board_info rcp_prog /usr/bin/scp
set_board_info ldflags   "-static"
set_board_info protocol "standard"
